CREATE database postfix;
CREATE USER 'postfix'@'localhost' IDENTIFIED BY 'MOT DE PASSE';
GRANT USAGE ON *.* TO 'postfix'@'localhost';
GRANT ALL PRIVILEGES ON postfix.* TO 'postfix'@'localhost';
